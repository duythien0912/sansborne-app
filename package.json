{
  "name": "react-next-boilerplate",
  "version": "1.1.0",
  "main": "index.js",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env ./node_modules/nodemon/bin/nodemon.js --watch server --experimental-worker ./server/app.js src",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node ./server/app.js",
    "test:unit": "cross-env NODE_ENV=test jest --coverage",
    "test": "npm run test:unit",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm run lint:js",
    "lint:js": "eslint src --fix",
    "lint:server": "eslint server --fix",
    "prettier": "prettier --config .prettierrc \"./**/*.+(js|json|jsx)\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.+(js|json|jsx)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@fortawesome/fontawesome-free": "5.14.0",
